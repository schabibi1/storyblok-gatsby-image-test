{
  "name": "gatsby-source-storyblok",
  "description": "Gatsby source plugin for building websites using the Storyblok headless CMS as a data source.",
  "version": "0.0.0-development",
  "author": "Alexander Feiglstorfer <af@storyblok.com>",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write 'src/**/*.js'"
  },
  "deprecated": false,
  "private": false,
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "storyblok",
    "headless-cms"
  ],
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "gatsby-source-filesystem": "^4.2.0",
    "json-stringify-safe": "^5.0.1",
    "storyblok-js-client": "^4.1.5"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "eslint": "7.13.0",
    "husky": "4.3.0",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "lodash.has": "^4.5.2",
    "prettier": "2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "storyblok-react": "^0.1.0"
  },
  "homepage": "https://www.storyblok.com",
  "repository": "https://github.com/storyblok/gatsby-source-storyblok"
}
